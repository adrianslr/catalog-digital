<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.cell.PropertyValueFactory?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Client.controller.PreavizController">
    <VBox AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">

    <TableView fx:id="table" editable="true" onMouseClicked="#Mouse">
    <columns>
    <TableColumn fx:id="nume" prefWidth="75.0" text="Nume">
        <cellValueFactory>
            <PropertyValueFactory property="firstNameCol" />
        </cellValueFactory>
    </TableColumn>
    <TableColumn fx:id="prenume" prefWidth="75.0" text="Prenume">
        <cellValueFactory>
            <PropertyValueFactory property="lastNameCol" />
        </cellValueFactory>
    </TableColumn>
    <TableColumn fx:id="nrAbsente" prefWidth="114.0" text="Numar Absente">
        <cellValueFactory>
            <PropertyValueFactory property="lastNameCol" />
        </cellValueFactory>
    </TableColumn>
    </columns>
    </TableView>
        <AnchorPane prefHeight="196.0" prefWidth="470.0">
            <padding>
                <Insets bottom="15.0" top="15.0" />
            </padding>
        </AnchorPane>

    </VBox>

</AnchorPane>
